			if(TERRITORIES[index][territory_VALID])
			{
				PLAYER_TEMP[playerid][pt_LAST_TERRITORY] = index;
				if(PI[playerid][pi_CREW])
				{
					if(TERRITORIES[index][territory_WAR])
					{
