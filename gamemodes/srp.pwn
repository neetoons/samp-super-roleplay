	mysql_format(srp_db, QUERY_BUFFER, sizeof QUERY_BUFFER, "UPDATE pweapons SET id_player = %d WHERE id = %d;", PI[to_playerid][pi_ID], PLAYER_WEAPONS[from_playerid][slot][player_weapon_DB_ID]);
