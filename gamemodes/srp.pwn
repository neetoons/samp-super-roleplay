GetVehicleRotation(vehicleid, &Float:rx, &Float:ry, &Float:rz) {
	new Float:qw, Float:qx, Float:qy, Float:qz;
	GetVehicleRotationQuat(vehicleid, qw, qx, qy, qz);
	rx = asin(2 * qy * qz - 2 * qx * qw);
